<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-9">
		<% questions.each do |question| %>
		  <div class="row well">
		    <div class="col-md-8">
		      <h4> <%= question.title %> </h4>
		    </div>
		    <div class="col-md-4">
		      <h4> <%= link_to 'Answer Question', question_path(question, user_id: params[:id]), class: 'btn btn-primary' %> </h4>
		    </div>
		  </div>
		<% end %>
	</div>
</div>