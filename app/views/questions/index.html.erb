<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h4>Title</h4>
    </div>
    <div class="col-md-3">
      <h4>Created By</h4>
    </div>
     <div class="col-md-3">
      <h4>Created At</h4>
    </div>
  </div>
  <% @questions.each do |question| %>
    <div class="row well">
      <div class="col-md-6">
        <h4> <%= question.title %> </h4>
        <h5> Category: <span class="badge"> <%= question.category.name %> </span></h5>
      </div>
      <div class="col-md-3">
        <h4> <%= question.user.email %> </h4>
      </div>
      <div class="col-md-3">
        <h4> <%= question.created_at %> </h4>
      </div>
    </div>
  <% end %>
</div>